{
    "name": "sierratecnologia/porteiro",
    "description": "porteiro",
    "type": "library",
    "keywords": [
        "porteiro"
    ],
    "homepage": "https://github.com/sierratecnologia/porteiro",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "laravel/passport": "^10.0",
        "laravel/socialite": "^5.0",
        "ricardosierra/laravel-haml": "^2.4",
        "ricardosierra/validate": "dev-master",
        "sierratecnologia/crypto": "dev-master",
        "sierratecnologia/muleta": "dev-master",
        "sierratecnologia/pedreiro": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "fzaninotto/faker": "^1.4",
        "laravel/browser-kit-testing": "^6.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.4",
        "laravel/dusk": "^6.2",
        "codedungeon/phpunit-result-printer": ">=0.27.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^4.0"
    },
    "suggest": {
        "netresearch/jsonmapper": "^3.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "authors": [
        {
            "name": "RiCa Soluções",
            "email": "contato@ricasolucoes.com.br",
            "homepage": "https://ricasolucoes.com.br"
        }
    ],
    "autoload": {
        "psr-4" : {
            "Porteiro\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
   "extra": {
      "laravel": {
          "providers": [
              "Porteiro\\PorteiroProvider"
          ],
          "aliases": {
              "Porteiro": "Porteiro\\Facades\\Porteiro"
          },
          "dont-discover": []
      }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
